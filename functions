#!/bin/bash

source config

git_update () {
	(cd "$code_base_loc/$1" && git pull)
	parsed_head=$(cd "$code_base_loc/$1" && git rev-parse HEAD)
	sed -e "s/$1_latest_commit/$1_latest_commit=\"$parsed_head\"/" config
}

wget_update () {
	eval code_url=\$"${1}_url"
	( cd "$code_base_loc/$1" && wget $code_url  )
}

github_latest_version () {
	echo "derp"
}
