#!/bin/bash

source config

git_update () {
	(cd "$code_base_loc/$1" && git pull)
	(cd "$code_base_loc/$1" && git rev-parse HEAD >> "$code_base_loc/$1_git_data")
}

wget_update () {
	eval code_url=\$"${1}_url"
	( cd "$code_base_loc/$1" && wget $code_url  )
}

github_latest_version () {
	echo "derp"
}
